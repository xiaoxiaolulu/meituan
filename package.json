{
  "name": "meituan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~5.0.8",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "better-scroll": "^1.15.2",
    "es6-promise": "^4.2.8",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "lib-flexible": "^0.3.2",
    "node-sass": "^4.13.1",
    "postcss-pxtorem": "^5.1.1",
    "sass-loader": "^7.0.3",
    "vant": "^2.5.3",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-pxtorem": {
        "rootValue": 37.5,
        "propList": [
          "*"
        ],
        "selectorBlackList": [
          "van-*"
        ]
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
